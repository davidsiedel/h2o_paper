@UseQt true;

//@MaterialProperty stress young;
//young.setGlossaryName("YoungModulus");
//@MaterialProperty real nu;
//nu.setGlossaryName("PoissonRatio");

@Parameter thermalexpansion alpha = 1e-6;
@Parameter young_mod = 150.e9;
@Parameter poisson_mod = 0.3;
@StaticVariable temperature Tref = 293.15;

// Lame Coefficients
@LocalVariable stress lambda,mu;

@InitLocalVariables{
//  lambda = computeLambda(young,nu);
//  mu = computeMu(young,nu);
  lambda = computeLambda(young_mod,poisson_mod);
  mu = computeMu(young_mod,poisson_mod);
}

@Integrator{
  constexpr const auto id = Stensor::Id();
  const auto lam_lam = poisson_mod * young_mod / ((1. + poisson_mod) * (1. - 2. * poisson_mod))
  const auto lam_mu = young_mod / (2. * (1. + poisson_mod))
  const auto eth = (alpha / 3) * (T + dT - Tref) * id;
  const auto e = eto + deto - eth;
  sig = lambda * trace(e) * id + 2 * mu * e;
}

@TangentOperator {
  Dt = lambda * Stensor4::IxI() + 2 * mu * Stensor4::Id();
}
